{% extends "Components/Innslag/element.html.twig" %}
{% block name %}
	{{ fancyCounter.next() }}.
	{% if innslag.getType().harTitler() %}
        {{ tittel.getNavn() }}
    {% else %}
        {{ innslag.getNavn() }}
    {% endif %}
	{% if config.get('tittel_visning') == 'tittel_og_navn' %}
        {% if innslag.getType().harTitler() %}
            - {{ innslag.getNavn() }}
        {% else %}
            <small>{{ innslag.getType().getNavn() }}</small>
        {% endif %}
	{% endif %}
{% endblock %}
