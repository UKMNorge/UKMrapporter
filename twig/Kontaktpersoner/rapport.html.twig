<div class="container-fluid">
	<div class="row">
		<div class="col-12 col-sm-8 col-md-7 col-lg-5 col-xl-4">
			<h2>
				Antall tilfeller for {{ renderData.getAttr('sesong')}}
			</h2>

			<table class="table table-striped col">
				<tr>
					<th width="*%"></th>
					<th width="22%">Totalt<br/>
					</th>
					<th width="22%">
						For påmeldte innslag
					</th>
					<th width="22%">
						For påbegynte innslag
					</th>
				</tr>
                <tr>
                    <th></th>
                    <th colspan="3" class="small" style="text-align: center; background: rgba(0,0,0,.04)">
						(antall kontaktpersoner)
                    </th>
				</tr>
				{% for gruppe in renderData.getGrupper() %}
					<tr>
						<td>{{gruppe.getOverskrift()}}</td>
						<td>{{gruppe.getAttr('totalt')}}</td>
						<td>{{gruppe.getAttr('pameldt')}}</td>
						<td>{{gruppe.getAttr('ikke_pameldt')}}</td>
					</tr>
				{% endfor %}
			</table>
		</div>
	</div>
</div>
