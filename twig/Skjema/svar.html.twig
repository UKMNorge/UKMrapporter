{% if type == 'kontakt' %}
	{{ svar.navn }}
	<br/>
	<small>
		{% if svar.mobil is not empty %}
			{% include "Components/mobil.html.twig" with {epost: svar.mobil } %}
		{% endif %}
		{{ svar.mobil and svar.epost is not empty ? '|' }}
		{% if svar.epost is not empty %}
			{% include "Components/epost.html.twig" with {epost: svar.epost, expand: true } %}
		{% endif %}
	</small>
{% elseif type == 'janei' %}
	{{ svar == 'true' ? 'Ja' : 'Nei' }}
{% elseif type == 'langtekst' %}
	{{ svar|nl2br }}
{% else %}
	{{ svar }}
{% endif %}
