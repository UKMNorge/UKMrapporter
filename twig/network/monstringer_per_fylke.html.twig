<h1>Mønstringer per fylke</h1>

{% for fylke in fylker %}
	<h2>{{ fylke.info.getNavn() }} <small>{{ fylke.info.getKommunerUtenGjester()|length }} kommuner</small></h2>
	<table class="table table-striped">
		<thead>
			<tr>
				<th>År</th>
				<th>Antall kommuner</th>
				<th>Komuner involvert <br />i en mønstring</th>
				<th>Antall arrangerte<br />mønstringer</th>
				<th>Kommuner i snitt per mønstring</th>
			</tr>
		</thead>
		<tbody>
			{% for i in startyear..stopyear %}
				<tr>
					<th><a href="?page=UKMrapport_admin&network=monstringer_i_fylket&fylke={{fylke.info.getId()}}&sesong={{i}}">{{ i }}</a></th>
					<td>
						{{ fylke.info.getKommunerUtenGjester()|length }}
					</td>
					<td>
						{{ fylke.monstringer[i].kommuner_registrert }}
					</td>
					<td>{{ fylke.monstringer[i].registrert }}</td>
					<td>
						{{ (fylke.monstringer[i].kommuner_registrert / fylke.monstringer[i].registrert)|round(1) }}
					</td>
				</tr>
			{% endfor %}
		</tbody>
	</table>
{% endfor %}